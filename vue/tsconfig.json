{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"lib": ["esnext", "esnext.asynciterable", "dom"],
		"esModuleInterop": true,
		"allowJs": true,
		"sourceMap": true,
		"strict": true,
		"noEmit": true,
		"baseUrl": ".",
		"types": ["@types/node", "@nuxt/types"]
	},
	"exclude": ["node_modules"]
}
